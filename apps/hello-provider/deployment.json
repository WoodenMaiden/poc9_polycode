{
    "apiVersion": "apps/v1",
    "kind": "Deployment",
    "metadata": {
      "name": "hello-provider"
    },
    "spec": {
      "replicas": 1,
      "selector": {
        "matchLabels": {
          "app": "hello-provider"
        }
      },
      "template": {
        "metadata": {
          "labels": {
            "app": "hello-provider"
          }
        },
        "spec": {
          "containers": [
            {
              "name": "hello-provider",
              "image": "hello-provider:latest",
              "imagePullPolicy": "Never",
              "ports": [
                {
                  "containerPort": 3333
                }
              ]
            }
          ]
        }
      }
    }
  }
  